<workflow>
    <ops>
        <operator>
            <input>"input.txt"</input>
            <run maxiters="5" numclusters="4">tfidf</run>
            <output>"tfidf-output.txt"</output>
        </operator>
        <operator>
            <input>"tfidf-output.txt"</input>
            <run numclusters="4">kmeans</run>
            <output>"tfidf-kmeans.txt"</output>
        </operator>

    </ops>
</workflow>
